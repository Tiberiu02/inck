{
  "name": "inck",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:443",
  "scripts": {
    "dev": "concurrently \"cd client && next dev -- -p 80\" \"node server/index.js\"",
    "dev-ubt": "concurrently \"cd client && next dev -- -p 3080\" \"sudo node server/index.js\"",
    "build": "cd client && next build",
    "start": "concurrently \"cd client && next start -- -p 3080\" \"sudo node server/index.js\"",
    "lint": "next lint"
  },
  "dependencies": {
    "concurrently": "^7.1.0",
    "express": "^4.17.3",
    "http": "0.0.1-security",
    "mongodb": "^4.5.0",
    "next": "12.1.5",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-icons": "^4.3.1",
    "socket.io": "^4.4.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.4",
    "eslint": "8.13.0",
    "eslint-config-next": "12.1.5",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.24"
  }
}
