    "@tailwindcss/line-clamp": "^0.3.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "concurrently": "^7.1.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "flag-icons": "^6.2.0",
    "http": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.5.0",
    "mongoose": "^6.3.0",
    "next": "12.1.5",
    "nodemailer": "^6.7.3",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-icons": "^4.3.1",
    "socket.io": "^4.4.1",
    "universal-cookie": "^4.0.4"
  },
  "devDependencies": {
